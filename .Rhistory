colpAUC = list()
rawWTW = list()
for(c in 1 : 2){
cond = conditions[c];
condName = conditionNames[c];
# input
if(condName == "HP") inputData = rawHPData else inputData = rawLPData
tMax = tMaxs[c]
trialTick = trialTicks[[condName]]
# dim
